# Configuracao do ambiente
# NODE_ENV="development" | "production" | "test"

# URLs da aplicacao
# MSW_LOCAL_DATABASE_URL="http://localhost:3005"
# API_URL="http://localhost:3000/api"
# BASE_URL="http://localhost:3000"

# Configuracao de Mock Service Worker
# NEXT_PUBLIC_API_MOCKING="enable" | "disable"

# Conexao com Supabase via connection pooling
# DATABASE_URL="postgresql://usuario:senha@host:porta/banco?pgbouncer=true"

# Conexao direta com o banco de dados (usada para migracoes)
# DIRECT_URL="postgresql://usuario:senha@host:porta/banco"

# Configuracoes do Discord OAuth
# DISCORD_CLIENT_ID="seu_client_id_aqui"
# DISCORD_SECRET="seu_secret_aqui"

# Chave secreta para JWT
# JWT_SECRETE="sua_chave_secreta_aqui"
